{% extends 'base.html.twig' %}

{# Inserts everything here into the similar called block in the parent template#}

{% block title %}Blog Index{% endblock %}

{% block body %}
    {% for  id, post in posts%}
        <p><a href="{{ path('blog_show', {'id': id}) }}">ID {{ id }}</a></p>
        <h3>{{ post.title }}</h3>
        <p>{{ post.text }}</p>
    {% endfor %}
{% endblock %}

{% extends 'base.html.twig' %}

{# Inserts everything here into the similar called block in the parent template #}

{% block title %}{{ post.title }}{% endblock %}

{% block body %}
        <p>ID {{ id }}</p>
        <h3>{{ post.title }}</h3>
        <p>{{ post.text }}</p>
        <p>{{ post.date|date('H:i:s') }}</p>
        <p>{{ post.price|price }}</p>
{% endblock %}
